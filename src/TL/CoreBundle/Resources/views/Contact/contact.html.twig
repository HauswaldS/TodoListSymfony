{% extends 'TLCoreBundle::layout.html.twig' %}
{% block content %}
{% if contactForm %}
    {{ form_start(contactForm) }}
    {{ form_row(contactForm.name) }}
    {{ form_row(contactForm.surname) }}
    {{ form_row(contactForm.email) }}
    {{ form_row(contactForm.send) }}
    {{ form_rest(contactForm) }}
    {{ form_end(contactForm) }}
{% endif %}
{% endblock%}